{
  "name": "Instagram-Clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.1",
    "firebase": "^9.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.1.0",
    "react-router-dom": "^6.4.4",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.2.4",
    "web-vitals": "^3.0.4"
  },
  "scripts": {
    "build:css": "postcss src.styles/tailwindcss.css -o src/styles/app.css",
    "watch:css": "postcss src/styles/tailwindcss.css -o src/styles/app.css --watch",
    "react-scripts:start": "sleep 5 && react-scripts start",
    "start": "run-p watch:css react-scripts:start",
    "build": "run-s build:css react-scripts:build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "postcss": "^8.4.19",
    "prop-types": "^15.8.1",
    "typescript": "^4.8.4"
  }
}
